{
    "name": "journii/jrn-permission",
    "description": "jrn-permission is an authorization library for php. It provides standard role based access control",
    "license": "MIT",
    "authors": [
        {
            "name": "Journii"
        }
    ],
    "keywords": [
        "acl",
        "authorization",
        "laravel",
        "permission",
        "php",
        "rbac",
        "roles"
    ],
    "require": {
        "php": ">=8.1", 
        "laravel/framework": "^11.0"
    }, 
    "require-dev": {
        "mockery/mockery": "^1.3.2",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^8.4|^9.0|^10.5|^11.5.3"
    },
    "autoload": {
        "psr-4": {
            "Jrn\\Rbac\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jrn\\Rbac\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
    
}